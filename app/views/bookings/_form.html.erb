<%#= form_for([@user, @booking]) do |f| %>
<%= form_with(model: [:user, booking], local: true) do |form| %>
  <p>
    <%= f.label 'start_time', 'Start time' %>
    <%= f.datetime_select :starts %>
  </p>
  <p>
    <%= f.label 'end_time', 'End time' %>
    <%= f.datetime_select :ends %>
  </p>
  <p>
    <%= f.label 'length', 'Length of booking in hours' %>
    <%= f.number_field 'length', min: 1 %>
  </p>
  <%= f.submit 'Submit' %>
<% end %>